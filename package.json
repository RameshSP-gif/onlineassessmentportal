{
  "name": "online-assessment-portal",
  "version": "1.0.0",
  "description": "Online Assessment Portal with React and Express",
  "scripts": {
    "start": "react-scripts start",
    "build": "react-scripts build",
    "test": "react-scripts test",
    "eject": "react-scripts eject",
    "server": "node api/index.js",
    "start:backend": "node api/index.js",
    "start:backend:5005": "cross-env PORT=5005 node api/index.js",
    "start:frontend:3003": "cross-env PORT=3003 react-scripts start",
    "dev": "concurrently \"react-scripts start\" \"node api/index.js\"",
    "dev:5005": "concurrently -n BE,FE -c cyan,magenta \"cross-env PORT=5005 node api/index.js\" \"cross-env PORT=3003 react-scripts start\"",
    "test:api": "jest --config jest.config.js --runInBand",
    "test:video:api": "node test-backend-video.js",
    "seed": "node api/seed.js",
    "seed-users": "node api/seed-users.js",
    "reset-users": "node api/reset-users.js",
    "test-login": "node api/test-login.js",
    "vercel-build": "CI=false npm run build"
  },
  "dependencies": {
    "axios": "^1.6.2",
    "bcrypt": "^5.1.1",
    "cors": "^2.8.5",
    "dotenv": "^16.3.1",
    "express": "^4.18.2",
    "jsonwebtoken": "^9.0.2",
    "mongodb": "^7.0.0",
    "mongoose": "^9.1.3",
    "multer": "^2.0.2",
    "nodemailer": "^7.0.12",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-router-dom": "^6.20.0",
    "react-scripts": "5.0.1",
    "sqlite3": "^5.1.6"
  },
  "devDependencies": {
    "concurrently": "^8.2.2",
    "cross-env": "^7.0.3",
    "nodemon": "^3.0.1",
    "jest": "^29.7.0",
    "mongodb-memory-server": "^10.1.4",
    "supertest": "^6.3.4"
  },
  "eslintConfig": {
    "extends": [
      "react-app"
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "proxy": "http://localhost:3002",
  "homepage": "./"
}
